{
  "name": "unplugin-build-meta",
  "version": "1.0.5",
  "description": "Provide build metadata as a virutal module",
  "type": "module",
  "author": {
    "name": "Lucas Nørgård",
    "email": "lucasnrgaard@gmail.com",
    "url": "https://luxass.dev"
  },
  "packageManager": "pnpm@10.28.2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luxass/unplugin-build-meta.git"
  },
  "bugs": "https://github.com/luxass/unplugin-build-meta/issues",
  "keywords": [
    "build-meta",
    "git",
    "sha",
    "unplugin",
    "vite",
    "webpack",
    "rollup",
    "esbuild",
    "nuxt",
    "astro"
  ],
  "workspaces": [
    "playground/*"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./astro": {
      "import": "./dist/astro.mjs",
      "require": "./dist/astro.cjs"
    },
    "./esbuild": {
      "import": "./dist/esbuild.mjs",
      "require": "./dist/esbuild.cjs"
    },
    "./farm": {
      "import": "./dist/farm.mjs",
      "require": "./dist/farm.cjs"
    },
    "./module": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./nuxt": {
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.cjs"
    },
    "./rolldown": {
      "import": "./dist/rolldown.mjs",
      "require": "./dist/rolldown.cjs"
    },
    "./rollup": {
      "import": "./dist/rollup.mjs",
      "require": "./dist/rollup.cjs"
    },
    "./rspack": {
      "import": "./dist/rspack.mjs",
      "require": "./dist/rspack.cjs"
    },
    "./vite": {
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    },
    "./webpack": {
      "import": "./dist/webpack.mjs",
      "require": "./dist/webpack.cjs"
    },
    "./types": {
      "types": "./types/index.d.ts"
    },
    "./types/*": {
      "types": "./types/*.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "prepack": "pnpm run generate-types",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "lint": "eslint .",
    "test": "vitest --run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit && pnpm -r typecheck",
    "play:vite": "pnpm run --filter @unplugin-build-meta/vite dev",
    "play:rspack": "pnpm run --filter @unplugin-build-meta/rspack dev",
    "generate-types": "node scripts/generate-types.js"
  },
  "peerDependencies": {
    "@nuxt/kit": ">=3",
    "@nuxt/schema": ">=3",
    "@rspack/core": "^1.0.13",
    "astro": ">=3",
    "esbuild": "*",
    "rolldown": ">=0.14.0",
    "rollup": ">=3",
    "vite": ">=3",
    "webpack": "^4 || ^5"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "@nuxt/schema": {
      "optional": true
    },
    "astro": {
      "optional": true
    },
    "esbuild": {
      "optional": true
    },
    "rollup": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "webpack": {
      "optional": true
    },
    "@rspack/core": {
      "optional": true
    },
    "rolldown": {
      "optional": true
    }
  },
  "dependencies": {
    "git-url-parse": "catalog:prod",
    "simple-git": "catalog:prod",
    "unplugin": "catalog:prod"
  },
  "devDependencies": {
    "@luxass/eslint-config": "catalog:dev",
    "@nuxt/kit": "catalog:dev",
    "@nuxt/schema": "catalog:dev",
    "@rspack/core": "catalog:dev",
    "@types/node": "catalog:dev",
    "esbuild": "catalog:dev",
    "eslint": "catalog:dev",
    "publint": "catalog:dev",
    "rolldown": "catalog:dev",
    "rollup": "catalog:dev",
    "tsdown": "catalog:dev",
    "typescript": "catalog:dev",
    "vite": "catalog:dev",
    "vitest": "catalog:dev",
    "vitest-testdirs": "catalog:dev",
    "webpack": "catalog:dev"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "require": "./dist/index.cjs",
        "import": "./dist/index.mjs"
      },
      "./astro": {
        "require": "./dist/astro.cjs",
        "import": "./dist/astro.mjs"
      },
      "./esbuild": {
        "require": "./dist/esbuild.cjs",
        "import": "./dist/esbuild.mjs"
      },
      "./farm": {
        "require": "./dist/farm.cjs",
        "import": "./dist/farm.mjs"
      },
      "./module": {
        "require": "./dist/module.cjs",
        "import": "./dist/module.mjs"
      },
      "./nuxt": {
        "require": "./dist/nuxt.cjs",
        "import": "./dist/nuxt.mjs"
      },
      "./rolldown": {
        "require": "./dist/rolldown.cjs",
        "import": "./dist/rolldown.mjs"
      },
      "./rollup": {
        "require": "./dist/rollup.cjs",
        "import": "./dist/rollup.mjs"
      },
      "./rspack": {
        "require": "./dist/rspack.cjs",
        "import": "./dist/rspack.mjs"
      },
      "./vite": {
        "require": "./dist/vite.cjs",
        "import": "./dist/vite.mjs"
      },
      "./webpack": {
        "require": "./dist/webpack.cjs",
        "import": "./dist/webpack.mjs"
      },
      "./package.json": "./package.json",
      "./types": {
        "types": "./types/index.d.ts"
      },
      "./types/*": {
        "types": "./types/*.d.ts"
      }
    }
  }
}
